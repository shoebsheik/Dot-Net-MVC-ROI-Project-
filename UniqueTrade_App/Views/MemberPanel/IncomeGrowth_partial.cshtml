@model IEnumerable<UniqueTrade_App.Models.WalletModel>
@using UniqueTrade_App.CommanFunction;
@{
    UserInfo userInfo = Common.CurrentUserInfo;
    ViewBag.Title = "IncomeGrowth";
    Layout = null;
}


    <div class="row">
        <div class="col-12">
            <div class="box">
                <div class="box-header with-border">
                    <h3 class="box-title">Roulette Income</h3>
                </div>
                <!-- /.box-header -->
                <div class="box-body">
                    <div class="table-responsive">
                        <table id="example1" class="table table-bordered table-striped">
                            <thead>
                                <tr>
                                    <th>Sr. No.</th>
                                    <th>Date</th>
                                    <th>Purchase Package</th>
                                    <th>Profit</th>
                                </tr>
                            </thead>
                            <tbody>
                                @{ int rows = 0;}
                                @foreach (var item in Model)
                                {
                                    <tr>
                                        <td>@(rows = rows + 1)</td>
                                        <td>@item.created_at</td>
                                        <td>$ @item.Package</td>
                                        <td>$ @item.amount</td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>


