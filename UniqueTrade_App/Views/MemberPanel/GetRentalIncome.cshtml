        <script>
            // Set the date we're counting down to
            function timer(tarikh, div) {
                //   $(div).text("loading ...")
                // alert("Timer Initialized !!!");
                debugger

              //  tarikh = "2023,08,9"

                var countDownDate = new Date(tarikh).getTime();

                // Update the count down every 1 second
                var x = setInterval(function ()
                {

                    // Get today's date and time
                    var now = new Date().getTime();

                    // Find the distance between now and the count down date
                    var distance = countDownDate - now;

                    // Time calculations for days, hours, minutes and seconds
                    var days = Math.floor(distance / (1000 * 60 * 60 * 24));
                    var hours = Math.floor((distance % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
                    var minutes = Math.floor((distance % (1000 * 60 * 60)) / (1000 * 60));
                    var seconds = Math.floor((distance % (1000 * 60)) / 1000);

                    // Display the result in the element with id="demo"
                    let timetxt = days + " D : " + hours + " H : " + minutes + " M : " + seconds + " S ";
                    $(div).children().eq(0).text(timetxt);
                    $(div).children().eq(0).css({ "font-size": "10px", "font-weight": "800" });

                    // If the count down is finished, write some text
                    if (distance < 0) {
                        clearInterval(x);
                      
                        $(div).html("<button> Click Here!!!<button>");
                        $(div).children().eq(0).remove();
                        $(div).children().eq(0).attr("type", "button");
                        $(div).children().eq(0).text("Click Here !!!");
                        $(div).children().eq(0).addClass("btn btn-dark");

                       // $(div).children().eq(0).data("data-plan", plan);
                        $(div).children().eq(0).attr("onclick", "return acivateRentalIncome(this)");
                        //  $(div).text("click Below");
                        // $(div).
                        //  $(div).children().eq(1).show();

                        // $(div).html("<button> Click Here!!!<button>");
                    }

                }, 1000);


            }

            function acivateRentalIncome(obj) {
                debugger

                var plan = $("#planType").data("plan");

               // alert("button Clicked !!!");

                Ajaxsend(plan);

            }






            function Ajaxsend(package) {
                debugger
                $.ajax({
                    url: window.location.origin + "/MemberPanel/SetRentalIncomeData",
                    type: 'post',
                    datatype: 'json',
                    data: { id : package },
                    success: RentalIncomeSuccess,
                    error: DisplayError
                });
            }

            function RentalIncomeSuccess(result) {

                debugger
                console.info(result);

                if (result.hasOwnProperty("STATUS") ) {

                    if (result.STATUS == "SUCCESS") {

                      //  alert("success !!!");
                       // setTimeout(() =>  window.location.reload() ,300);
                        var plan = $("#planType").data("plan");
                        anonymous(plan);
                    }
                    else if (result.STATUS == "NOTTODAY")
                    {
                        alert("try some other day !!!")
                    }
                    else if (result.STATUS == "NOTTOPUP") {
                        alert("try some other day !!!")
                    }

                } else if (result = "404") {
                    alert("Failure !!!")
                }

            }
