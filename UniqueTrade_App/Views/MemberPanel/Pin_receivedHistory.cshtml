@model UniqueTrade_App.Models.PinTransferModel
@using UniqueTrade_App.CommanFunction;

@{
    UserInfo userInfo = Common.CurrentUserInfo;
    ViewBag.Title = "Pin Transfer history";
    Layout = "~/Views/Shared/User_Layout.cshtml";
}


<section id="main-content" class=" ">
    <div class="wrapper main-wrapper row">

        <div class="col-xs-12">
            <div class="page-title">
                <div class="hidden-xs">
                    <ol class="breadcrumb">
                        <li>
                            <a href="/MemberPanel/index"><i class="fa fa-home"></i>Home</a>
                        </li>
                        <li>
                            <a href="#">Pin</a>
                        </li>
                        <li class="active">
                            <strong>Pin Received History</strong>
                        </li>
                    </ol>
                </div>

            </div>
        </div>

        <div class="clearfix"></div>
        <!-- MAIN CONTENT AREA STARTS -->

        <div class="clearfix"></div>
        <div class="col-12 mt-25">
            <div class="card">
                <div class="card-header">
                    <h3 class="card-title">Pin Received History</h3>
                </div>
                <div class="card-body">
                    <table id="example" class="table table-striped table-bordered table-responsive" style="width:100%">
                        <thead>
                            <tr>
                                <th>Sr.No</th>
                                <th> Date</th>
                                <th>Received From</th>
                                <th>Number</th>
                            </tr>


                        </thead>
                        <tbody>
                            @{ int rows = 0;}
                            @foreach (var item in Model.trnsferHistory)
                            {
                                <tr role="row" class="odd">
                                    <td>@(rows = rows + 1)</td>
                                    <td>@item.TF_Date</td>
                                    <td>@item.Memb_Name</td>
                                    <td>@item.Pin</td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>

        </div>

        <div class="clearfix"></div>
        <!-- MAIN CONTENT AREA ENDS -->

    </div>
</section>








































