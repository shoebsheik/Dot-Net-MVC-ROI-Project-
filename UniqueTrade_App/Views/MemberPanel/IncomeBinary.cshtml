@model IEnumerable<UniqueTrade_App.Models.BinaryReportModel>
@using UniqueTrade_App.CommanFunction;
@{
    UserInfo userInfo = Common.CurrentUserInfo;
    ViewBag.Title = "IncomeBinary";
    Layout = "~/Views/Shared/_User_Layout.cshtml";
}
<!--#INCLUDE FILE = "header.asp"-->
<!-- Page Content  -->
<div id="content-page" class="content-page">
    <div class="container-fluid">


        <div class="row">
            <div class="col-sm-12">
                <div class="iq-card iq-card-block iq-card-stretch iq-card-height">
                    <div class="iq-card-body">
                        <div class="iq-advance-course d-flex align-items-center justify-content-between">
                            <div class="left-block">
                                <div class="d-flex align-items-center">
                                    <div>
                                        <h4 class="text-primary pb-2 fw-bold">Earning Report</h4>
                                        <ul class="breadcrumbs ">
                                            <li class="nav-home ">
                                                <a class="" href="/MemberPanel/index">
                                                    <i class="fa fa-home"> Home</i>
                                                </a>
                                            </li>

                                            <li class="nav-item">
                                                <a class="" href="#">Binary Income</a>
                                            </li>

                                        </ul>
                                    </div>
                                </div>
                            </div>
                            <div class="right-block position-relativ">
                                <a href="/MemberPanel/" class="btn btn-primary">Direct Income</a>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>



        <div class="row">
            <div class="col-12">
                <div class="card">
                    <div class="card-header bg-primary">
                        <h4 class="card-title text-white">Binary Income</h4>
                    </div>
                    <div class="card-body">
                        <table id="example" class="table table-striped table-bordered" style="width:100%">
                            <thead>
                                <tr>
                                    <th>Sr. No.</th>
                                    <th>Date</th>
                                    <th>Amount</th>
                                    @*<th>Income </th>*@
                                </tr>
                            </thead>
                            <tbody>
                                @{ int rows = 0;}
                                @foreach (var item in Model)
                                {
                                    <tr>
                                        @*<td>@(rows = rows + 1)</td>
                                            <td>@item.created_at</td>
                                            <td>@item.amount</td>*@
                                        @*<td>@item.provide_id</td>*@
                                        @*<td>TRX @item.amount</td>*@
                                    </tr>
                                }
                            </tbody>

                        </table>
                    </div>
                </div>
            </div>
        </div>








    </div>
</div>
      </div>
<!-- Wrapper END -->
